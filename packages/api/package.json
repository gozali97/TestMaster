{
  "name": "@testmaster/api",
  "version": "1.0.0",
  "main": "./dist/index.js",
  "scripts": {
    "build": "tsc",
    "dev": "tsx watch src/index.ts",
    "start": "node dist/index.js",
    "start:fresh": "npm run kill-port && npm start",
    "stop": "powershell -File stop-api.ps1",
    "restart": "npm run stop && npm run build && npm start",
    "kill-port": "powershell -File kill-port.ps1",
    "clean": "rm -rf dist",
    "lint": "eslint src --ext .ts",
    "test": "playwright test",
    "test:ui": "playwright test --ui",
    "test:debug": "playwright test --debug",
    "test:headed": "playwright test --headed",
    "test:report": "playwright show-report",
    "test:codegen": "playwright codegen",
    "migrate": "sequelize-cli db:migrate",
    "migrate:undo": "sequelize-cli db:migrate:undo"
  },
  "dependencies": {
    "@aws-sdk/client-s3": "^3.917.0",
    "@elastic/elasticsearch": "^9.2.0",
    "@testmaster/shared": "*",
    "@testmaster/test-engine": "*",
    "aws-sdk": "^2.1518.0",
    "axios": "^1.6.2",
    "bcrypt": "^5.1.1",
    "bullmq": "^5.1.5",
    "cors": "^2.8.5",
    "dotenv": "^16.3.1",
    "express": "^4.18.2",
    "express-rate-limit": "^7.1.5",
    "helmet": "^7.1.0",
    "ioredis": "^5.3.2",
    "jimp": "^1.6.0",
    "joi": "^17.11.0",
    "jsonwebtoken": "^9.0.2",
    "moment": "^2.30.1",
    "moment-timezone": "^0.5.48",
    "mongoose": "^8.0.3",
    "multer": "^1.4.5-lts.1",
    "mysql2": "^3.7.0",
    "nodemailer": "^6.9.7",
    "openai": "^6.7.0",
    "passport": "^0.7.0",
    "passport-github2": "^0.1.12",
    "passport-google-oauth20": "^2.0.0",
    "passport-jwt": "^4.0.1",
    "pixelmatch": "^7.1.0",
    "pngjs": "^7.0.0",
    "prom-client": "^15.1.3",
    "sequelize": "^6.35.2",
    "sharp": "^0.34.4",
    "socket.io": "^4.6.1",
    "swagger-jsdoc": "^6.2.8",
    "swagger-ui-express": "^5.0.0",
    "winston": "^3.11.0"
  },
  "devDependencies": {
    "@playwright/test": "^1.56.1",
    "@types/bcrypt": "^5.0.2",
    "@types/cors": "^2.8.17",
    "@types/express": "^4.17.21",
    "@types/jsonwebtoken": "^9.0.5",
    "@types/multer": "^1.4.11",
    "@types/node": "^24.9.1",
    "@types/nodemailer": "^6.4.14",
    "@types/passport": "^1.0.16",
    "@types/passport-jwt": "^4.0.0",
    "@types/swagger-ui-express": "^4.1.6",
    "playwright": "^1.56.1",
    "sequelize-cli": "^6.6.2",
    "tsx": "^4.7.0",
    "typescript": "^5.3.3"
  }
}
